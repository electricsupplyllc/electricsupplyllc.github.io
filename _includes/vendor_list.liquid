<div style="display: flex; flex-wrap: wrap">
    {% for vendor in site.data.vendors %}
        <div style="margin: 1em auto">
            <a href="{{ vendor.url }}">
                <img src="{{ vendor.logo }}" alt="{{ vendor.name }}" style="max-width: 256px;
  -webkit-filter: drop-shadow(1px 1px 0 #666)
                  drop-shadow(-1px -1px 0 #666);
  filter: drop-shadow(1px 1px 0 #666)
          drop-shadow(-1px -1px 0 #666);
}">
            </a>
        </div>
{% endfor %}
</div>